<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data>

        <record id="res_partner_available_exchanges" model="ir.ui.view">
            <field name="name">Partner Exchange View</field>
            <field name="model">res.partner</field>
            <field name="inherit_id" ref="base.view_partner_form"/>
            <field name="arch" type="xml">
                <notebook position="inside">
                    <page name="exchanges" string="EDI Exchanges">
                        <field name="available_exchange_ids"/>
                    </page>
                </notebook>
            </field>
        </record>

    </data>
</odoo>